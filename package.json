{
  "name": "ElectronBoilerplateReact",
  "productName": "Cosplay",
  "companyName": "ND",
  "private": true,
  "version": "0.0.1",
  "description": "Electron Boilerplate React",
  "main": "main.js",
  "scripts": {
    "start": "node server.js --env=dev",
    "start-elec": "better-npm-run start-elec",
    "package": "better-npm-run package",
    "build": "better-npm-run build",
    "test": "karma start",
    "test:watch": "karma start --autoWatch=true --singleRun=false",
    "posttest": "npm run lint",
    "serve": "node server.js --env=dev",
    "serve:dist": "node server.js --env=dist",
    "dist": "npm run copy & webpack --env=dist",
    "lint": "eslint ./src",
    "copy": "copyfiles -f ./src/index.html  ./dist",
    "copystyles": "copyfiles  -f ./src/styles/*.css ./dist/styles",
    "clean": "rimraf dist/*",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags"
  },
  "betterScripts": {
    "start-elec": {
      "command": "electron ././",
      "env": {
        "HOT": 1,
        "NODE_ENV": "dev"
      }
    },
    "package": {
      "command": "node package.js --env=production",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build": {
      "command": "npm run copy &npm run copystyles & webpack --config webpack.config.js --progress --profile --colors --env=production",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "keywords": [],
  "author": "caofb",
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-eslint": "^5.0.0-beta4",
    "babel-loader": "^6.0.0",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-3": "^6.3.13",
    "better-npm-run": "0.0.5",
    "bower-webpack-plugin": "^0.1.9",
    "chai": "^3.2.0",
    "copyfiles": "^0.2.1",
    "css-loader": "^0.23.0",
    "electron-packager": "^5.1.1",
    "electron-prebuilt": "^0.36.0",
    "eslint": "^1.2.1",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-react": "^3.3.0",
    "file-loader": "^0.8.4",
    "glob": "^6.0.0",
    "isparta-instrumenter-loader": "^1.0.0",
    "json-loader": "^0.5.4",
    "karma": "^0.13.9",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^0.5.3",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.1",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-phantomjs-shim": "^1.1.1",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.7.0",
    "minimist": "^1.2.0",
    "mocha": "^2.2.5",
    "null-loader": "^0.1.1",
    "open": "0.0.5",
    "phantomjs": "^1.9.18",
    "react-hot-loader": "^1.2.9",
    "redux-devtools": "^3.0.0",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.1",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.0",
    "webpack-dev-server": "^1.12.0",
    "webpack-target-electron-renderer": "^0.3.0"
  },
  "dependencies": {
    "async": "^1.5.0",
    "babel-polyfill": "^6.3.14",
    "cheerio": "^0.19.0",
    "core-js": "^1.2.6",
    "electron-debug": "^0.5.1",
    "jquery": "^2.1.4",
    "lodash": "^3.10.1",
    "material-ui": "^0.13.4",
    "mkdirp": "^0.5.1",
    "mv": "^2.1.1",
    "normalize.css": "^3.0.3",
    "react": "^0.14.0",
    "react-addons-test-utils": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.0",
    "redux": "^3.0.4",
    "redux-promise": "^0.5.0",
    "redux-thunk": "^1.0.0",
    "request": "^2.67.0",
    "rimraf": "^2.5.0",
    "sequelize": "^3.15.0",
    "whatwg-fetch": "^0.10.1"
  }
}
